{
  "name": "@gift/init",
  "private": true,
  "scripts": {
    "start": "NODE_OPTIONS='--enable-source-maps' node ./dist/main.js",
    "build": "builder",
    "dev": "nodemon --exec 'node -r esbuild-register ./src/main.ts' -e .ts",
    "lint": "npm run lint:prettier && npm run lint:ts && npm run lint:eslint",
    "lint:fix": "npm run lint:prettier:fix && npm run lint:eslint:fix",
    "lint:eslint": "eslint ./src",
    "lint:ts": "tsc --noEmit",
    "lint:eslint:fix": "eslint --fix ./src",
    "lint:prettier": "prettier --check ./src",
    "lint:prettier:fix": "prettier --write ./src"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "nestjs-rmq": "^2.7.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@gift/builder": "*",
    "@gift/eslint-config": "*",
    "@gift/tsconfig": "*"
  }
}
